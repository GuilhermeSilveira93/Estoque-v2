{
  "extends": [
    "@kalilasc/eslint-config/nextjs",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react",
    "react-native",
    "@typescript-eslint",
    "react-hooks",
    "eslint-plugin-import-helpers"
],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "no-unused-vars": "warn",
    "@typescript-eslint/no-var-requires": "off"
  }
}